{
	"name": "dotnetcore-acquisition-library",
	"description": "A library to acquire .NET Core tooling.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.33.0"
	},
	"main": "./dist/extension.js",
	"types": "./dist/extension.d.ts",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "npm run clean && tsc -p ./",
		"watch": "npm run clean && tsc -watch -p ./",
		"test": "npm run compile && npm run unit-test && npm run functional-test",
		"functional-test": "node ./dist/test/functional/runTest.js",
		"unit-test": "mocha -u tdd -- dist/test/unit/**.test.js",
		"clean": "rimraf dist"
	},
	"devDependencies": {
		"@types/chai": "^4.2.4",
		"@types/mocha": "5.2.6",
		"@types/node": "12.0.0",
		"@types/rimraf": "2.0.2",
		"@types/shelljs": "0.8.3",
		"@types/vscode": "^1.33.0",
		"chai": "^4.2.0",
		"glob": "^7.1.5",
		"mocha": "^4.1.0",
		"rimraf": "2.6.3",
		"shelljs": "0.8.3",
		"tslint": "5.16.0",
		"typescript": "3.4.5",
		"vscode-test": "^1.2.2"
	}
}
